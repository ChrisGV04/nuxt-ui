{
  "name": "@cgv.web/nuxt-ui",
  "version": "0.0.1",
  "description": "A Nuxt 3 module containing the most common components used at CGV WEB",
  "repository": "ChrisGV04/nuxt-ui",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "author": {
    "name": "Christian Gil Villase√±or",
    "email": "christian@chrisgvdev.com",
    "url": "https://chrisgvdev.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run prepack && changelogen --release && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "^3.4.1"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.7.13",
    "@nuxt/module-builder": "^0.3.0",
    "@nuxt/schema": "^3.4.1",
    "@nuxtjs/tailwindcss": "^6.6.6",
    "@types/node": "^18",
    "@vueuse/core": "^10.0.2",
    "@vueuse/nuxt": "^10.0.2",
    "changelogen": "^0.5.3",
    "nuxt": "^3.4.1",
    "nuxt-icon": "^0.3.3",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7"
  }
}
